  entry  = {"Manual"},
  author = {"Eiling, Ward Bernd"},
  doi = {"10.5281/zenodo.14674098"},
  title = {"wardeiling/roundR2: Version 0.2.0 - First release"},
  version = {"v0.2.0"},
  year = {"2025"},
